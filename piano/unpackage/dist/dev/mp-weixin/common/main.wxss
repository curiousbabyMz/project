/*每个页面公共css */
.flex {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
button,
button::after {
  width: auto;
  line-height: auto;
  padding: 0;
  margin: 0;
  background: none;
  border: none;
  font-size: 0;
}
.button-hover {
  background: none;
}
@font-face {
  font-family: 'iconfont';
  /* project id 1449661 */
  src: url('//at.alicdn.com/t/font_1449661_18qbk5m4tcli.eot');
  src: url('//at.alicdn.com/t/font_1449661_18qbk5m4tcli.eot?#iefix') format('embedded-opentype'), url('//at.alicdn.com/t/font_1449661_18qbk5m4tcli.woff2') format('woff2'), url('//at.alicdn.com/t/font_1449661_18qbk5m4tcli.woff') format('woff'), url('//at.alicdn.com/t/font_1449661_18qbk5m4tcli.ttf') format('truetype'), url('//at.alicdn.com/t/font_1449661_18qbk5m4tcli.svg#iconfont') format('svg');
}
[class*=icon] {
  font-family: "iconfont" !important;
  font-size: 16rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  text-align: center;
  line-height: 1.2em;
}
.icon_person1:before {
  content: "\E62E";
}
.icon_arrowR:before {
  content: '\E65C';
}

