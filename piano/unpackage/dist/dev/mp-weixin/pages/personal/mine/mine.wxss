.container .personal {
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-flow: column nowrap;
      -ms-flex-flow: column nowrap;
          flex-flow: column nowrap;
  background: #FFFFFF;
  position: relative;
}
.container .personal .login {
  margin-top: 100rpx;
  z-index: 1;
}
.container .personal .login .headImg {
  background: #FFFFFF;
  border-radius: 50%;
  font-size: 150rpx;
  color: #5daffb;
  border: 8rpx solid #5daffb;
}
.container .personal .login .name {
  margin-top: 30rpx;
  font-size: 28rpx;
  color: #007AFF;
}
.container .personal .sea,
.container .personal .sea::before,
.container .personal .sea::after {
  position: absolute;
  top: -1850rpx;
  width: 2000rpx;
  height: 2000rpx;
  border-radius: 44% 46%;
  -webkit-animation: 10s sea0 forwards infinite linear;
          animation: 10s sea0 forwards infinite linear;
  background: #8aedffdc;
}
.container .personal .sea:before {
  background: #ffe6b152;
  -webkit-animation-duration: 12s;
          animation-duration: 12s;
  -webkit-animation-name: 'sea1';
          animation-name: 'sea1';
  top: 0;
  content: "";
}
.container .personal .sea::after {
  background: #5daffb7c;
  -webkit-animation-duration: 14s;
          animation-duration: 14s;
  -webkit-animation-name: 'sea2';
          animation-name: 'sea2';
  top: 0;
  content: "";
}
@-webkit-keyframes sea0 {
0% {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
100% {
    -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
}
}
@keyframes sea0 {
0% {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
100% {
    -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
}
}
@-webkit-keyframes sea1 {
0% {
    -webkit-transform: rotate(60deg);
            transform: rotate(60deg);
}
100% {
    -webkit-transform: rotate(420deg);
            transform: rotate(420deg);
}
}
@keyframes sea1 {
0% {
    -webkit-transform: rotate(60deg);
            transform: rotate(60deg);
}
100% {
    -webkit-transform: rotate(420deg);
            transform: rotate(420deg);
}
}
@-webkit-keyframes sea2 {
0% {
    -webkit-transform: rotate(120deg);
            transform: rotate(120deg);
}
100% {
    -webkit-transform: rotate(480deg);
            transform: rotate(480deg);
}
}
@keyframes sea2 {
0% {
    -webkit-transform: rotate(120deg);
            transform: rotate(120deg);
}
100% {
    -webkit-transform: rotate(480deg);
            transform: rotate(480deg);
}
}

