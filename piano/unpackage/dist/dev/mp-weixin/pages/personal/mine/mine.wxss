.flex {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.container .personal {
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-flow: column nowrap;
          flex-flow: column nowrap;
  background: #FFFFFF;
  position: relative;
}
.container .personal .login {
  margin-top: 100rpx;
  z-index: 1;
  overflow: visible;
}
.container .personal .login .head {
  position: relative;
  width: 1em;
  height: 1em;
  font-size: 150rpx;
}
.container .personal .login .head .headImg {
  position: absolute;
  left: 0;
  top: 0;
  background: #FFFFFF;
  border-radius: 50%;
  width: inherit;
  height: inherit;
  font-size: 150rpx;
  color: #5daffb;
  box-shadow: 0 0 5rpx 5rpx #4481bb62;
}
.container .personal .login .head .headIn {
  -webkit-transition: all 0.25s 0.25s ease-out;
  transition: all 0.25s 0.25s ease-out;
  -webkit-transform: none;
          transform: none;
}
.container .personal .login .head .headOut {
  -webkit-transition: all 0.25s ease-out;
  transition: all 0.25s ease-out;
  -webkit-transform: rotateY(90deg);
          transform: rotateY(90deg);
}
.container .personal .login .name {
  margin-top: 30rpx;
  font-size: 28rpx;
  color: #007AFF;
}
.container .personal .sea,
.container .personal .sea::before,
.container .personal .sea::after {
  position: absolute;
  top: -1850rpx;
  width: 2000rpx;
  height: 2000rpx;
  border-radius: 44% 46%;
  -webkit-animation: 10s sea0 forwards infinite linear;
          animation: 10s sea0 forwards infinite linear;
  background: #8aedffdc;
}
.container .personal .sea:before {
  background: #ffe6b152;
  -webkit-animation-duration: 12s;
          animation-duration: 12s;
  -webkit-animation-name: 'sea1';
          animation-name: 'sea1';
  top: 0;
  content: "";
}
.container .personal .sea::after {
  background: #5daffb7c;
  -webkit-animation-duration: 11s;
          animation-duration: 11s;
  -webkit-animation-name: 'sea2';
          animation-name: 'sea2';
  top: 0;
  content: "";
}
.container .menu {
  padding: 0 50rpx;
  font-size: 34rpx;
  line-height: 100rpx;
  color: #333333;
}
.container .menu > .item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  border-top: 1rpx solid #e2e2e2;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.container .menu > .item text {
  font-size: 34rpx;
}
.container .menu > .item:nth-of-type(1) {
  border: none;
}
@-webkit-keyframes sea0 {
0% {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
100% {
    -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
}
}
@keyframes sea0 {
0% {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
100% {
    -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
}
}
@-webkit-keyframes sea1 {
0% {
    -webkit-transform: rotate(60deg);
            transform: rotate(60deg);
}
100% {
    -webkit-transform: rotate(420deg);
            transform: rotate(420deg);
}
}
@keyframes sea1 {
0% {
    -webkit-transform: rotate(60deg);
            transform: rotate(60deg);
}
100% {
    -webkit-transform: rotate(420deg);
            transform: rotate(420deg);
}
}
@-webkit-keyframes sea2 {
0% {
    -webkit-transform: rotate(120deg);
            transform: rotate(120deg);
}
100% {
    -webkit-transform: rotate(480deg);
            transform: rotate(480deg);
}
}
@keyframes sea2 {
0% {
    -webkit-transform: rotate(120deg);
            transform: rotate(120deg);
}
100% {
    -webkit-transform: rotate(480deg);
            transform: rotate(480deg);
}
}

